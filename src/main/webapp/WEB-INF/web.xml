<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>z-index</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>manager.html</welcome-file>
		<welcome-file>manager.htm</welcome-file>
		<welcome-file>manager.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>

	<servlet>
		<servlet-name>travelService</servlet-name>
		<servlet-class>servlet.TravelServiceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>travelService</servlet-name>
		<url-pattern>/travelService</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateMealHistoryStatus</servlet-name>
		<servlet-class>servlet.UpdateMealHistoryStatus</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateMealHistoryStatus</servlet-name>
		<url-pattern>/UpdateMealHistoryStatus</url-pattern>
	</servlet-mapping>
</web-app>